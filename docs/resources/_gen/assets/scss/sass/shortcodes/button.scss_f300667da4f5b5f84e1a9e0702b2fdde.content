    border-color: #28a745; }
  button.btn.warning {
    color: black;
    background-color: #ffc107;
    border-color: #ffc107; }
  button.btn.danger {
    color: white;
    background-color: #dc3545;
    border-color: #dc3545; }
  button.btn.danger {
    color: white;
    background-color: #dc3545;
    border-color: #dc3545; }
  button.btn.secondary {
    color: white;
