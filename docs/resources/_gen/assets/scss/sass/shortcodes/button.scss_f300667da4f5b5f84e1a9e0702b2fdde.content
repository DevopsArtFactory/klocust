button.btn {
  border: 1px solid transparent;
  padding: .375rem .75rem;
  font-size: 1rem;
  line-height: 1.5;
  border-radius: .25rem;
  cursor: pointer;
  color: #fff;
  background-color: #007bff;
  border-color: #007bff; }
  button.btn a {
    color: inherit !important; }
  button.btn.primary {
